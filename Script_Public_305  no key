local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("سكربت عام | Script Public", "DarkTheme") 

-- Credit Tab
local Tab = Window:NewTab("Credit")
local Section = Tab:NewSection("1 : Magstnic")
local Section2 = Tab:NewSection("2 : M5tr")

-- Wall Hop Tab
local Tab2 = Window:NewTab("Wall Hop | وال هوب")
local SectionWH = Tab2:NewSection("Wall Hop Controls")
local UIS = game:GetService("UserInputService")

-- Camera Functions
local function flickCamera90Degrees()
    local currentCam = workspace.CurrentCamera
    local initialPos = currentCam.CFrame.Position
    local initialRot = currentCam.CFrame - initialPos
    currentCam.CFrame = CFrame.new(initialPos) * CFrame.Angles(0, math.rad(90), 0) * initialRot
end

local function flickCameraBack90Degrees()
    local currentCam = workspace.CurrentCamera
    local initialPos = currentCam.CFrame.Position
    local initialRot = currentCam.CFrame - initialPos
    currentCam.CFrame = CFrame.new(initialPos) * CFrame.Angles(0, math.rad(-90), 0) * initialRot
end

local function flickCameraSequence()
    flickCamera90Degrees()
    wait(0.03)
    flickCameraBack90Degrees()
end

-- Wall Hop Buttons
SectionWH:NewButton("WH 1 (Q)", "PC ONLY!", function()
    UIS.InputBegan:Connect(function(input, processed)
        if not processed and input.KeyCode == Enum.KeyCode.Q then
            flickCameraSequence()
        end
    end)
end)

SectionWH:NewButton("WH 2 (E)", "PC ONLY!", function()
    UIS.InputBegan:Connect(function(input, processed)
        if not processed and input.KeyCode == Enum.KeyCode.E then
            flickCameraSequence()
        end
    end)
end)

